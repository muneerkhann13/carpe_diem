{  
   "filter":[  
	{  
         "name":"response_log",
         "filename":"response.log",
         "format":"[%D %T] [%L] (%S) %M",
         "default_lvl":"INFO",
         "rotate":true,
         "max_size":2048,
         "max_lines":100,
         "rotate_daily":true,
         "message_fields":[  
            {  
               "tag":"request_id",
			"data_type":"int",
               "size":"10"
            },
            {  
               "tag":"mdn",
			"data_type":"string",
               "size":"15"
            },
            {  
               "tag":"request_text",
			"data_type":"string",
               "size":"5000"
            }
         ]
      },
	{  
         "name":"request_log",
         "filename":"kamal.log",
         "format":"[%D %T] [%L] (%S) %M",
         "default_lvl":"INFO",
         "rotate":true,
         "max_size":2048,
         "max_lines":100,
         "rotate_daily":true,
         "message_fields":[  
            {  
               "tag":"request_id",
			"data_type":"int",
               "size":"10"
            },
            {  
               "tag":"mdn",
			"data_type":"string",
               "size":"15"
            },
            {  
               "tag":"request_text",
			"data_type":"string",
               "size":"5000"
            }
         ]
      }
   ]
}